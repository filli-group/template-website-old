<!doctype html>
<html lang="de">

<head>
    <title>{{ title }} | {{ brand }}</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <!-- Fonts and icons -->
    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons"/>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@5.11.1/css/all.min.css"
          integrity="sha256-PbSmjxuVAzJ6FPvNYsrXygfGhNJYyZ2GktDbkMBqQZg=" crossorigin="anonymous">
    <!-- Material CSS -->
    <link href="{{ css|default('https://cdn.jsdelivr.net/gh/Filli-Group/material-kit@2.0.6/assets/css/material-kit.min.css') }}"
          rel="stylesheet"/>
    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="180x180" href="https://cdn.statically.io/img/i.imgur.com/{{ favicon_apple_touch_icon }}.png">
    <link rel="icon" type="image/png" sizes="32x32" href="https://cdn.statically.io/img/i.imgur.com/{{ favicon_icon_32 }}.png">
    <link rel="icon" type="image/png" sizes="16x16" href="https://cdn.statically.io/img/i.imgur.com/{{ favicon_icon_16 }}.png">
    <link rel="manifest"
          href="https://cdn.statically.io/gl/filli-group/img/raw/{{ favicon_manifest }}/site.webmanifest">
    <link rel="mask-icon"
          href="https://cdn.statically.io/gl/filli-group/img/raw/{{ favicon_mask_icon }}/safari-pinned-tab.svg"
          color="#5bbad5">
    <link rel="shortcut icon" href="https://cdn.filli-group.com/{{ favicon_shortcut_icon }}/img/favicon.ico">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-config"
          content="https://cdn.statically.io/gl/filli-group/img/raw/{{ favicon_msapplication_config }}/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
    <!-- Meta Tags -->
    <meta name='keywords' content='{{ meta_keywords }}'>
    <meta name='description' content='{{ meta_description }}'>
    <meta name='author' content='Ursin Filli, ursin.filli@Filli-IT.ch'>
    <meta name='copyright' content='Filli Group (Einzelunternehmen) and/or its affiliates'>
    <meta name='reply-to' content='ursin.filli@Filli-IT.ch'>
    <meta name='revisit-after' content='1 week'>
</head>

{% if error is defined %}
<body class="off-canvas-sidebar">
{% else %}
<body>
{% endif %}

{% include 'navigation.twig' %}

{% if error is defined %}
    <div class="wrapper wrapper-full-page">
        <div class="page-header error-page header-filter"
             style="background-image: url({{ bg_img|default('https://cdn.statically.io/img/i.imgur.com/HQlcoa7.jpg') }})">
            {% block content_error %}{% endblock %}

            {% include 'footer.twig' %}
        </div>
    </div>
{% else %}
    <div class="page-header header-filter" data-parallax="true"
         style="background-image: url({{ bg_img|default('https://cdn.statically.io/img/i.imgur.com/LQtyM3F.jpg') }})">
        <div class="container">
            <div class="row">
                <div class="col-md-8 ml-auto mr-auto">
                    <div class="brand text-center">
                        <h1>{{ page_title }}</h1>
                        <h3 class="title text-center">{{ page_sub_title }}</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="main main-raised">
        <div class="container">
            <div class="section{% if legal is not defined %} text-center{% endif %}">
                {% block content %}{% endblock %}
            </div>
        </div>
    </div>

    {% include 'footer.twig' %}
{% endif %}

